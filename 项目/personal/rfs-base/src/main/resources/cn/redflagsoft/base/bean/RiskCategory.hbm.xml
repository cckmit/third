<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.redflagsoft.base.bean.RiskCategory" table="Risk_Category">
		<id name="type" column="ID" type="java.lang.Integer">
			<generator class="assigned"/>
		</id>
		<property name="name" column="NAME" type="java.lang.String"/>
		
		<property name="tableName" column="TABLE_NAME" type="java.lang.String"/>
		
		<property name="status" column="STATUS" type="byte"/>
	</class>

</hibernate-mapping>
