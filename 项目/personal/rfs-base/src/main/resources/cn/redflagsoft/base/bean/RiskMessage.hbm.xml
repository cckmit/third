<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class  name="cn.redflagsoft.base.bean.RiskMessage" table="RISK_MESSAGE">
		<id name="id" column="ID" type="java.lang.Long">
			<generator class="assigned"/>
		</id>
		
		<property name="type" column="TP" type="int"/>
		
		<property name="clerkId" column="CLERKID" type="java.lang.Long"/>
		
		<property name="objectType" column="OBJECT_TYPE" type="int"/>
		
		<property name="refType" column="REF_TYPE" type="int"/>
		
		<property name="dutyerType" column="DUTYER_TYPE" type="short"/>
		
		<property name="ruleId" column="RULEID" type="java.lang.Long"/>
		
		<property name="status" column="STATUS" type="byte"/>
		
		<property name="remark" column="REMARK" type="java.lang.String"/>
	</class>
</hibernate-mapping>
