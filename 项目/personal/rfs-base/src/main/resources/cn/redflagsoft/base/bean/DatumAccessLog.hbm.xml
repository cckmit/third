<?xml version="1.0"  encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.redflagsoft.base.bean.DatumAccessLog" table="DATUM_ACCESS_LOG">
		<id name="id" column="ID" type="java.lang.Long">
			<generator class="assigned"/>
		</id>
		
		<property name="type" column="TP" type="int"/>
		
		<property name="status" column="STATUS" type="byte"/>
		
		<property name="remark" column="REMARK" type="java.lang.String"/>
		
		<property name="creator" column="CREATOR" type="java.lang.Long"/>
		
		<property name="creationTime" column="CTIME" type="java.sql.Timestamp"/>
		
		<property name="modifier" column="MODIFIER" type="java.lang.Long"/>
		
		<property name="modificationTime" column="MTIME" type="java.sql.Timestamp"/>
		
		<property name="datumId" column="DATUM_ID" type="java.lang.Long"/>
		
		<property name="accessTime" column="ACCESS_TIME" type="java.sql.Timestamp"/>
		
		<property name="accessType" column="ACCESS_TYPE" type="java.lang.Integer"/>
		
		<property name="accessorId" column="ACCESSOR_ID" type="java.lang.Long"/>
		
		<property name="accessorName" column="ACCESSOR_NAME" type="java.lang.String"/>
		
	</class>
</hibernate-mapping>