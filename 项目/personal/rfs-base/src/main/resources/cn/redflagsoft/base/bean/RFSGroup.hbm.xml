<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.redflagsoft.base.bean.RFSGroup" table="RFS_GROUP">
		<id name="id" column="ID" type="java.lang.Long">
			<generator class="assigned"/>
		</id>
		
		<property name="category" column="CATEGORY" type="byte"/>
		
		<property name="name" column="NAME" type="java.lang.String"/>
		
		<property name="abbr" column="ABBR" type="java.lang.String"/>
		
		<property name="canInsert" column="CAN_INSERT" type="java.lang.Byte"/>
		
		<property name="canUpdate" column="CAN_UPDATE" type="java.lang.Byte"/>
		
		<property name="canDelete" column="CAN_DELETE" type="java.lang.Byte"/>
		
		<property name="memberNum" column="MEMBER_NUM" type="java.lang.Short"/>
		
		<property name="note" column="NOTE" type="java.lang.String"/>
		
		<property name="status" column="STATUS" type="byte"/>
		
		<property name="creator" column="CREATOR" type="java.lang.Long"/>
		
		<property name="creationTime" column="CTIME" type="java.sql.Timestamp"/>
		
		<property name="modifier" column="MODIFIER" type="java.lang.Long"/>
		
		<property name="modificationTime" column="MTIME" type="java.sql.Timestamp"/>
		
		<property name="remark" column="REMARK" type="java.lang.String"/>
		
		<property name="displayOrder" column="DISPLAY_ORDER" type="java.lang.Integer"/>
		
	</class>
</hibernate-mapping>
	