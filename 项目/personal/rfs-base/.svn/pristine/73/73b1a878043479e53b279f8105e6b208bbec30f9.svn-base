<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.redflagsoft.base.security.Menu" table="SEC_MENU">
        <id name="authority" column="AUTHORITY" type="java.lang.String" length="50">
            <generator class="assigned"/>
        </id>
        
        <!--<property name="authority" column="AUTHORITY" type="java.lang.String"/>-->
        
        <property name="application" column="APPLICATION" type="java.lang.String"/>
        
        <property name="name" column="NAME" type="java.lang.String"/>
        
        <property name="groupId" column="GROUPID" type="java.lang.Long"/>
        
        <property name="displayOrder" column="DISPLAY_ORDER" type="int"/>
        
        <property name="icon" column="ICON" type="java.lang.String"/>
        <property name="image" column="IMAGE" type="java.lang.String"/>
        <property name="logo" column="LOGO" type="java.lang.String"/>
        <property name="label2" column="LABEL" type="java.lang.String"/>
	</class>
</hibernate-mapping>