<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.redflagsoft.base.bean.OrgCategoryToOrgGroup" table="ORG_CAT_TO_ORG_GROUP">
		<id name="id" column="ID" type="java.lang.Long">
			<generator class="assigned"/>
		</id>
		
		<property name="orgCategoryId" column="ORG_CAT_ID" type="java.lang.Long"/>
		
		<property name="orgGroupId" column="ORG_GROUP_ID" type="java.lang.Long"/>
		
		<property name="deleter" column="DELETER" type="java.lang.Long"/>
	
		<property name="deleteTime" column="DTIME" type="java.sql.Timestamp"/>
		
		
		
		<property name="type" column="TP" type="int"/>
		
		<property name="status" column="STATUS" type="byte"/>
		
		<property name="creator" column="CREATOR" type="java.lang.Long"/>
		
		<property name="creationTime" column="CTIME" type="java.sql.Timestamp"/>
		
		<property name="modifier" column="MODIFIER" type="java.lang.Long"/>
		
		<property name="modificationTime" column="MTIME" type="java.sql.Timestamp"/>
		
	</class>
</hibernate-mapping>
	