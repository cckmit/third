<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.redflagsoft.base.bean.BizMatter" table="BIZ_MATTER">
        <id name="sn" column="SN" type="java.lang.Long">
            <generator class="assigned"/>
        </id>
        
        <property name="type" column="TP" type="int"/>

        <property name="jobSN" column="JOBSN" type="java.lang.Long"/>
        
        <property name="taskSN" column="TASKSN" type="java.lang.Long"/>
        
        <property name="workSN" column="WORKSN" type="java.lang.Long"/>
        
        <property name="processSN" column="PROCESSSN" type="java.lang.Long"/>

        <property name="matterID" column="MATTERID" type="java.lang.Long"/>

        <property name="displayOrder" column="DISPLAY_ORDER" type="byte"/>
        
		<property name="status" column="STATUS" type="byte"/>
		
		<property name="creationTime" column="CTIME" type="java.sql.Timestamp"/>
		
		<property name="modificationTime" column="MTIME" type="java.sql.Timestamp"/>
		
		<property name="remark" column="REMARK" type="java.lang.String"/>        
    </class>
</hibernate-mapping>
