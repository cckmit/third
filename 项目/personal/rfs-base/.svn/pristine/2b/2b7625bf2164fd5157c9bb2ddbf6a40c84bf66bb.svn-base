<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	<class name="cn.redflagsoft.base.bean.SignCardNotice" table="SIGN_CARD_NOTICE">
		<id name="id" column="ID" type="java.lang.Long">
			<generator class="assigned" />
		</id>	
		<property name="signCardId" column="SIGNCARD_ID" type="java.lang.Long"/>
		
		<property name="noticeDutyerId" column="NOTICE_DUTYERID" type="java.lang.Long"/>
		
		<property name="noticeDutyerName" column="NOTICE_DUTYERNAME" type="java.lang.String"/>
		
		<property name="noticeType" column="NOTICE_TYPE" type="byte"/>
		
		<property name="noticeWay" column="NOTICE_WAY" type="java.lang.String"/>
		
		<property name="noticeContent" column="NOTICE_CONTENT" type="java.lang.String"/>
		
		<property name="noticeCreator" column="NOTICE_CREATOR" type="java.lang.Long"/>
		
		<property name="noticeActualTime" column="NOTICE_ACTUALTIME" type="java.sql.Timestamp"/>
		
		<property name="noticeTime" column="NOTICE_TIME" type="java.sql.Timestamp"/>
	
		<property name="noticeRemark" column="NOTICE_REMARK" type="java.lang.String"/>
	
		<property name="type" column="TP" type="int"/>
		
		<property name="status" column="STATUS" type="byte"/>
		
		<property name="remark" column="REMARK" type="java.lang.String"/>
			
		<property name="creator" column="CREATOR" type="java.lang.Long"/>
			
		<property name="creationTime" column="CTIME" type="java.sql.Timestamp"/>
			
		<property name="modifier" column="MODIFIER" type="java.lang.Long"/>
			
		<property name="modificationTime" column="MTIME" type="java.sql.Timestamp"/>
	</class>
</hibernate-mapping>
