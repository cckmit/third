package cn.redflagsoft.base.service;

/**
 * 消息没有支持的发送器。
 * @author Alex Lin(lcql@msn.com)
 *
 */
public class SmsgNoSupportedSenderException extends SmsgException {

	/**
	 * 
	 */
	private static final long serialVersionUID = -7043896125494262235L;

	/**
	 * 
	 */
	public SmsgNoSupportedSenderException() {
		super();
	}

	/**
	 * @param message
	 * @param cause
	 */
	public SmsgNoSupportedSenderException(String message, Throwable cause) {
		super(message, cause);
	}

	/**
	 * @param message
	 */
	public SmsgNoSupportedSenderException(String message) {
		super(message);
	}

	/**
	 * @param cause
	 */
	public SmsgNoSupportedSenderException(Throwable cause) {
		super(cause);
	}

}
