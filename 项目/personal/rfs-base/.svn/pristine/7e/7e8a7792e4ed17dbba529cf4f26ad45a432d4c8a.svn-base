package cn.redflagsoft.base.bean;



/**
 * @author 
 *
 */
public class FileNo extends VersionableBean{
	private static final long serialVersionUID = 4910510883304811909L;
	private String uid;                                   //唯一标识
	private String description;                          //描述
	private String template;                            //模板
	private boolean autoIncrement = false;             //是否自增长
	private int incrementValue = 0;                   //自增长当前值
	private int incrementStringLength = 0;           //自增长字串长度
	private String lastFileNo;                      //最后一次生成的文号
	private int displayOrder = 0;                  //显示序号
	
	public FileNo(){
		
	}
	/**
	 * @return the uid
	 */
	public String getUid() {
		return uid;
	}

	/**
	 * @param uid the uid to set
	 */
	public void setUid(String uid) {
		this.uid = uid;
	}

	/**
	 * @return the description
	 */
	public String getDescription() {
		return description;
	}

	/**
	 * @param description the description to set
	 */
	public void setDescription(String description) {
		this.description = description;
	}

	/**
	 * @return the template
	 */
	public String getTemplate() {
		return template;
	}

	/**
	 * @param template the template to set
	 */
	public void setTemplate(String template) {
		this.template = template;
	}

	/**
	 * @return the autoIncrement
	 */
	public boolean isAutoIncrement() {
		return autoIncrement;
	}

	/**
	 * @param autoIncrement the autoIncrement to set
	 */
	public void setAutoIncrement(boolean autoIncrement) {
		this.autoIncrement = autoIncrement;
	}

	/**
	 * @return the incrementValue
	 */
	public int getIncrementValue() {
		return incrementValue;
	}

	/**
	 * @param incrementValue the incrementValue to set
	 */
	public void setIncrementValue(int incrementValue) {
		this.incrementValue = incrementValue;
	}

	/**
	 * @return the lastFileNo
	 */
	public String getLastFileNo() {
		return lastFileNo;
	}

	/**
	 * @param lastFileNo the lastFileNo to set
	 */
	public void setLastFileNo(String lastFileNo) {
		this.lastFileNo = lastFileNo;
	}

	/**
	 * @return the displayOrder
	 */
	public int getDisplayOrder() {
		return displayOrder;
	}

	/**
	 * @param displayOrder the displayOrder to set
	 */
	public void setDisplayOrder(int displayOrder) {
		this.displayOrder = displayOrder;
	}
	/**
	 * @return the incrementStringLength
	 */
	public int getIncrementStringLength() {
		return incrementStringLength;
	}
	/**
	 * @param incrementStringLength the incrementStringLength to set
	 */
	public void setIncrementStringLength(int incrementStringLength) {
		this.incrementStringLength = incrementStringLength;
	}
}
