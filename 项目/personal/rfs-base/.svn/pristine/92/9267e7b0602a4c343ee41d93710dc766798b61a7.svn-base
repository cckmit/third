package cn.redflagsoft.base.bean;

import java.io.Serializable;


public class DatumCategoryAndMatterDatum implements Serializable{

	private static final long serialVersionUID = 2885817302414786014L;
	
	private MatterDatum matterDatum;
	private DatumCategory datumCategory;
	public DatumCategoryAndMatterDatum(DatumCategory dc, MatterDatum md){
		this.datumCategory = dc;
		this.matterDatum = md;
	}
	public MatterDatum getMatterDatum() {
		return matterDatum;
	}
	public void setMatterDatum(MatterDatum matterDatum) {
		this.matterDatum = matterDatum;
	}
	public DatumCategory getDatumCategory() {
		return datumCategory;
	}
	public void setDatumCategory(DatumCategory datumCategory) {
		this.datumCategory = datumCategory;
	}
}
