package cn.redflagsoft.base.audit;


/**
 * Thrown when an {@link Auditor} is asked to audit a method call which
 * it was not intended for.
 */
public class NotAuditableException extends RuntimeException {

    /**
	 * 
	 */
	private static final long serialVersionUID = -4887901236798683076L;

	public NotAuditableException() {
        super();
    }

    public NotAuditableException(String msg) {
        super(msg);
    }

    public NotAuditableException(String msg, Throwable throwable) {
        super(msg, throwable);
    }

    public NotAuditableException(Throwable throwable) {
        super(throwable);
    }

}
