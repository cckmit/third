<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.redflagsoft.base.bean.BaseTaskBean" table="BASE_TASK_BEAN">
		<id name="id" column="ID" type="java.lang.Long">
			<generator class="assigned"/>
		</id>
		
		<property name="baseTask" column="BASE_TASK" type="java.lang.String"/>
		
		<property name="paramType" column="PARAM_TYPE" type="java.lang.String"/>
		
		<property name="paramValue" column="PARAM_VALUE" type="string"/>
		
		<property name="executeTime" column="EXEC_TIME" type="java.sql.Timestamp"/>
		
		<property name="executeNumber" column="EXEC_NUM" type="int"/>
		
		<property name="scheduleExecuteTime" column="SCHEDULE_EXEC_TIME" type="java.sql.Timestamp"/>
		
		<property name="result" column="RESULT_" type="java.lang.String"/>
		
		<property name="taskInstance" column="TASK_INSTANCE" type="binary"/>
		
		<property name="status" column="STATUS" type="byte"/>
		
		<property name="creator" column="CREATOR" type="java.lang.Long"/>
		
		<property name="creationTime" column="CTIME" type="java.sql.Timestamp"/>
		
		<property name="modifier" column="MODIFIER" type="java.lang.Long"/>
		
		<property name="modificationTime" column="MTIME" type="java.sql.Timestamp"/>
		
		<property name="remark" column="REMARK" type="java.lang.String"/>
	</class>
</hibernate-mapping>
