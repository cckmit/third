<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.redflagsoft.base.bean.MsgAttachments" table="MSG_ATTACHMENTS">
		<id name="id" column="ID" type="java.lang.Long">
			<generator class="assigned"/>
		</id>
		
		<property name="type" column="TP" type="int"/>
		
		<property name="sendingMsgId" column="SMSGID" type="java.lang.Long"/>
		
		<property name="fileId" column="FILEID" type="java.lang.Long"/>
	</class>
</hibernate-mapping>