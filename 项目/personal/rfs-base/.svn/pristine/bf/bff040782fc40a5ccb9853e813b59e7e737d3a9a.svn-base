package cn.redflagsoft.base.vo.statistics;

import java.io.Serializable;

public class StatisticGradeVO implements Serializable{
	public static final long serialVersionUID = 1782798254185410974L;
	public static final int WARNING_GRADE = 0;
	public static final int RED_GRADE = 10;
	public static final int YELLOW_GRADE = 20;
	public static final int NORMAL_GRADE = 30;
	
	private int warning;
	private int red;
	private int yellow;
	private int normal;
	
	/**
	 * @return the warning
	 */
	public int getWarning() {
		return warning;
	}
	
	/**
	 * @param warning the warning to set
	 */
	public void setWarning(int warning) {
		this.warning = warning;
	}
	
	/**
	 * @return the red
	 */
	public int getRed() {
		return red;
	}
	
	/**
	 * @param red the red to set
	 */
	public void setRed(int red) {
		this.red = red;
	}
	
	/**
	 * @return the yellow
	 */
	public int getYellow() {
		return yellow;
	}
	
	/**
	 * @param yellow the yellow to set
	 */
	public void setYellow(int yellow) {
		this.yellow = yellow;
	}
	
	/**
	 * @return the normal
	 */
	public int getNormal() {
		return normal;
	}
	
	/**
	 * @param normal the normal to set
	 */
	public void setNormal(int normal) {
		this.normal = normal;
	}
}