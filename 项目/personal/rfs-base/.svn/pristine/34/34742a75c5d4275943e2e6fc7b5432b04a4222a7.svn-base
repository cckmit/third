<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="cn.redflagsoft.base.bean.People" table="PEOPLE">
		<id name="id" column="ID" type="java.lang.Long">
			<generator class="assigned"/>
		</id>
		
		<property name="objectID" column="OBJECTID" type="java.lang.Long"/>
				
		<property name="type" column="TP" type="int"/>		
		
		<property name="code" column="CODE" type="java.lang.String"/>
		
		<property name="name" column="NAME" type="java.lang.String"/>
		
		<property name="alias" column="ALIAS" type="java.lang.String"/>
		
		<property name="birthDay" column="BIRTH_DAY" type="java.sql.Timestamp"/>
		
		<property name="paperType" column="PAPER_TYPE" type="java.lang.String"/>
		
		<property name="paperNo" column="PAPERNO" type="java.lang.String"/>
		
		<property name="paperElse" column="PAPER_ELSE" type="byte"/>
		
		<property name="nativeAddr" column="NATIVE_ADDR" type="java.lang.String"/>
		
		<property name="homeTelNo" column="HOME_TELNO" type="java.lang.String"/>
		
		<property name="homeAddr" column="HOME_ADDR" type="java.lang.String"/>
		
		<property name="mobNo" column="MOBNO" type="java.lang.String"/>
		
		<property name="faxNo" column="FAXNO" type="java.lang.String"/>
		
		<property name="emailAddr" column="EMAIL_ADDR" type="java.lang.String"/>
		
		<property name="workEntity" column="WORK_ENTITY" type="java.lang.String"/>
		
		<property name="workAddr" column="WORD_ADDR" type="java.lang.String"/>
		
		<property name="telNo" column="TELNO" type="java.lang.String"/>
		
		<property name="privateMobNo" column="PRIVATE_MOBNO" type="java.lang.String"/>
		
		<property name="status" column="STATUS" type="byte"/>
		
		<property name="creator" column="CREATOR" type="java.lang.Long"/>
		
		<property name="creationTime" column="CTIME" type="java.sql.Timestamp"/>
		
		<property name="modifier" column="MODIFIER" type="java.lang.Long"/>
		
		<property name="modificationTime" column="MTIME" type="java.sql.Timestamp"/>
		
		<property name="remark" column="REMARK" type="java.lang.String"/>
	</class>
</hibernate-mapping>