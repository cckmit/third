<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.redflagsoft.base.bean.MatterDatum" table="Matter_DATUM">
		<id name="id" column="ID" type="java.lang.Long">
			<generator class="assigned"/>
		</id>
		<property name="type" column="TP" type="int"/>
		
		<property name="category" column="CATEGORY" type="byte"/>
		
		<property name="taskType" column="TASK_TYPE" type="int"/>
		
		<property name="workType" column="WORK_TYPE" type="int"/>
		
		<property name="processType" column="PROCESS_TYPE" type="int"/>
		
		<property name="matterID" column="MATTERID" type="java.lang.Long"/>
		
		<property name="datumType" column="DATUM_TYPE" type="java.lang.Long"/>
		
		<property name="count" column="CNT" type="short"/>
		
		<property name="orgNum" column="ORG_NUM" type="byte"/>
		
		<property name="copyNum" column="COPY_NUM" type="byte"/>
		
		<property name="selected" column="SELECTED" type="byte"/>
		
		<property name="displayOrder" column="DISPLAY_ORDER" type="byte"/>
		
		<property name="taskAction" column="TASK_ACTION" type="byte"/>
		
		<property name="objectAction" column="OBJECT_ACTION" type="byte"/>
		
		<property name="status" column="STATUS" type="byte"/>
		
		<property name="creator" column="CREATOR" type="java.lang.Long"/>
		
		<property name="creationTime" column="CTIME" type="java.sql.Timestamp"/>
		
		<property name="modifier" column="MODIFIER" type="java.lang.Long"/>
		
		<property name="modificationTime" column="MTIME" type="java.sql.Timestamp"/>
		
		<property name="remark" column="REMARK" type="java.lang.String"/>
		
		<property name="fileNoField" column="FILE_NO_FIELD" type="java.lang.String"/>
		
		<property name="fileIdField" column="FILE_ID_FIELD" type="java.lang.String"/>
		
		<property name="bizAction" column="BIZ_ACTION" type="byte"/>
		
	</class>
</hibernate-mapping>
