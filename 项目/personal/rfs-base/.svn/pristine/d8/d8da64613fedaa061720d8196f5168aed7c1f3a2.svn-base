<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	<class name="cn.redflagsoft.base.bean.SignCardSurvey" table="SIGN_CARD_SURVEY">
		<id name="id" column="ID" type="java.lang.Long">
			<generator class="assigned" />
		</id>	
		<property name="signCardId" column="SIGNCARD_ID" type="java.lang.Long"/>
		
		<property name="surveyDutyerId" column="SURVEY_DUTYERID" type="java.lang.Long"/>
		
		<property name="surveyDutyerName" column="SURVEY_DUTYERNAME" type="java.lang.String"/>
		
		<property name="surveySummary" column="SURVEY_SUMMARY" type="java.lang.String"/>
		
		<property name="surveyFileNo" column="SURVEY_FILENO" type="java.lang.String"/>
		
		<property name="surveyTruth" column="SURVEY_TRUTH" type="java.lang.String"/>
		
		<property name="dealType" column="DEAL_TYPE" type="byte"/>
		
		<property name="surveyCreator" column="SURVEY_CREATOR" type="java.lang.Long"/>
		
		<property name="surveyActualTime" column="SURVEY_ACTUALTIME" type="java.sql.Timestamp"/>
		
		<property name="surveyTime" column="SURVEY_TIME" type="java.sql.Timestamp"/>
	
		<property name="surveyRemark" column="SURVEY_REMARK" type="java.lang.String"/>
	
		<property name="type" column="TP" type="int"/>
		
		<property name="status" column="STATUS" type="byte"/>
		
		<property name="remark" column="REMARK" type="java.lang.String"/>
			
		<property name="creator" column="CREATOR" type="java.lang.Long"/>
			
		<property name="creationTime" column="CTIME" type="java.sql.Timestamp"/>
			
		<property name="modifier" column="MODIFIER" type="java.lang.Long"/>
			
		<property name="modificationTime" column="MTIME" type="java.sql.Timestamp"/>
	</class>
</hibernate-mapping>
