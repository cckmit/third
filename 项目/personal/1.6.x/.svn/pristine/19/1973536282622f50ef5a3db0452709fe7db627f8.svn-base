package org.opoo.apps.security;

import java.util.Date;

/**
 * 属性可变的用户对象。
 *
 */
public interface MutableUser extends User {
	
	void setLoginAddress(String loginAddress);

	void setLoginCount(int loginCount);

	void setOnlineStatus(OnlineStatus onlineStatus);

	void setLastLoginTime(Date lastLoginTime);
	
	void setLoginTime(Date loginTime);

	void setTryLoginCount(int tryLoginCount);
}
