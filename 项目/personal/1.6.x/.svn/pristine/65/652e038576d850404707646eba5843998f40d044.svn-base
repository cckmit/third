package org.opoo.apps.conversion.doc;

import java.io.File;
import java.io.Serializable;

import org.opoo.apps.conversion.ConversionRevision;

public class DocConversionContext implements Serializable
{
	private static final long serialVersionUID = 239771650151460813L;
	
	private ConversionRevision revision;
	private File file;
	private File pdf;
	
	public DocConversionContext() {
		super();
	}

	public DocConversionContext(ConversionRevision revision, File file, File pdf) {
		super();
		this.revision = revision;
		this.file = file;
		this.pdf = pdf;
	}

	public ConversionRevision getRevision() {
		return revision;
	}

	public File getFile() {
		return file;
	}

	public File getPdf() {
		return pdf;
	}
}
