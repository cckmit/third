package org.opoo.ndao.domain;


/**
 * 主键是 String 类型的实体类基类。
 * @author Alex Lin(alex@opoo.org)
 *
 */
public abstract class StringKeyEntity extends Entity<String> implements StringKeyDomain {

	/**
	 * 
	 */
	private static final long serialVersionUID = -646731207006448228L;

	public String getKey() {
		return getId();
	}

	public void setKey(String key) {
		setId(key);
	}
}
