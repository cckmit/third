package org.opoo.apps;

public interface AppsObject {
	 /**
     * Retrieve the unique identifier for the apps object, or -1 if the object hasn't yet been 
     * added/inserted into the system.
     *
     * @return the unique identifier for the apps object.
     */
    Long getId();

    /**
     * Return the object type of the apps object.
     *
     * @return the object type of the apps object.
     */
    int getObjectType();
}
